		<!-- MAIN PANEL -->
		<div role="main">

			<!-- RIBBON -->
			<div id="ribbon">

				<span class="ribbon-button-alignment"> 
					<span id="refresh" class="btn btn-ribbon" data-action="resetWidgets" data-title="refresh"  rel="tooltip" data-placement="bottom" data-original-title="<i class='text-warning fa fa-warning'></i> Warning! This will reset all your widget settings." data-html="true">
						<i class="fa fa-refresh"></i>
					</span> 
				</span>

				<!-- breadcrumb -->
				<ol class="breadcrumb">
					<li><a href="/home">Inicio</a></li><li><a href="/stores">Tiendas</a></li><li>Nueva Tienda</li>
				</ol>
				<!-- end breadcrumb -->
				<span class="ribbon-button-alignment pull-right">
					<a href="/tickets"><span id="search" class="btn btn-ribbon hidden-xs" data-title="search"><i class="fa fa-lg fa-align-justify"></i> Ver Tickets</span></a>
				</span> 				
				<!-- You can also add more buttons to the
				ribbon for further usability

				Example below:

				<span class="ribbon-button-alignment pull-right">
				<span id="search" class="btn btn-ribbon hidden-xs" data-title="search"><i class="fa-grid"></i> Change Grid</span>
				<span id="add" class="btn btn-ribbon hidden-xs" data-title="add"><i class="fa-plus"></i> Add</span>
				<span id="search" class="btn btn-ribbon" data-title="search"><i class="fa-search"></i> <span class="hidden-mobile">Search</span></span>
				</span> -->

			</div>
			<!-- END RIBBON -->

			<!-- MAIN CONTENT -->
			<div id="content">


			<div class="row">
				<div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
					<h1 class="page-title txt-color-blueDark">
						
						<!-- PAGE HEADER -->
						<i class="fa-fw fa fa-pencil-square-o"></i> 
							Tienda
						<span>>  
							Crear nueva
						</span>
					</h1>
				</div>
				
			</div>

			<!-- widget grid -->
			<section id="widget-grid" class="">


				<!-- START ROW -->

				<div class="row">

					<!-- NEW COL START -->
					<article class="col-sm-12 col-md-12 col-lg-12">
						
						<!-- Widget ID (each widget will need unique ID)-->
						<div class="jarviswidget" id="wid-id-3" data-widget-editbutton="false" data-widget-custombutton="false">
							<!-- widget options:
								usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
								
								data-widget-colorbutton="false"	
								data-widget-editbutton="false"
								data-widget-togglebutton="false"
								data-widget-deletebutton="false"
								data-widget-fullscreenbutton="false"
								data-widget-custombutton="false"
								data-widget-collapsed="true" 
								data-widget-sortable="false"
								
							-->
							<header>
								<span class="widget-icon"> <i class="fa fa-edit"></i> </span>
								<h2>Crear Tienda </h2>				
								
							</header>

							<!-- widget div-->
							<div>
								
								<!-- widget edit box -->
								<div class="jarviswidget-editbox">
									<!-- This area used as dropdown edit box -->
									
								</div>
								<!-- end widget edit box -->
								
								<!-- widget content -->
								<div class="widget-body no-padding">
									
									<form id="order-form" class="smart-form" method ="post" action="/newStore">
										<header>
											Información de la Tienda
										</header>

										<fieldset>
											<div class="row">
												<label class="col-md-1 control-label" style= "margin-left: 20px;width: 50px;">Nombre:</label>	
												<section class="col col-5" style= "padding-left: 20px;">
													<label class="input"> <i class="icon-append fa fa-briefcase"></i>
														<input type="text" id="storeName" name="storeName" placeholder="Ingrese el nombre con el que desea identificar la tienda.">
													</label>
												</section>
												<label class="col-md-1 control-label" style= "margin-left: 10px;margin-top: 5px;width: 60px;">Ciudad:</label>
												<section class="col col-3">
													<label class="select">
														<select name="city_id">
														<option value="0" selected="" disabled>Seleccione la ciudad</option>
															{{#each cities}}
																<option value="{{id}}">{{city}}</option>
															{{/each}}
														</select> <i></i> </label>
												</section>		                                                
											</div>

											<div class="row">
												<label class="col-md-1 control-label" style= "margin-left: 20px;width: 60px;">Dirección:</label>												
												<section class="col col-3">
													<label class="input"> <i class="icon-append fa fa-user"></i>
														<input type="text" id="address" name="address" placeholder="Ingrese la dirección">
													</label>
												</section>                                              
												<label class="col-md-1 control-label" style= "margin-left: 20px;width: 60px;">Telefono:</label>												
												<section class="col col-3">                                          
													<label class="input"> <i class="icon-append fa fa-user"></i>
														<input type="text" id="phone" data-mask="(999)999-9999" name="phone" placeholder="Ingrese el teléfono">
													</label>
												</section>                                                
												<label class="col-md-1 control-label" style= "margin-left: 10px;width: 60px;">Email:</label>												
												<section class="col col-3">                                          
													<label class="input"> <i class="icon-append fa fa-user"></i>
														<input type="text" id="email"  name="email" placeholder="Ingrese el correo electrónico">
													</label>
												</section>   																							
											</div>																																																													
										</fieldset>
										<div class="note" class="col-md-3 control-label" style= "margin-left: 20px">
											<strong>*</strong> Todos los campos son requeridos.
										</div>										
										{{#if systemAdmin}}
										<fieldset>
										<legend>
											Administrar Ticket
										</legend>
										<label class="col-md-1 control-label" style= "margin-left: 10px;margin-top: 5px;width: 60px;">Estado:</label>											
											<div class="row">
												<section class="col col-5">
													<label class="select">
														<select name="interested">
															<option value="0" selected="" disabled="">Estado del Ticket</option>
															<option value="1">Pendiente</option>
															<option value="2">Asignado</option>
															<option value="3">Finalizado</option>
															<option value="4">Cancelado</option>
														</select> <i></i> </label>
												</section>
												<label class="col-md-1 control-label" style= "margin-left: 5px;margin-top: 5px;width: 60px;">Asignar a:</label>
												<section class="col col-5">
													<label class="select">
														<select name="budget">
															<option value="0" selected="" disabled="">Budget</option>
															<option value="1">less than 5000$</option>
															<option value="2">5000$ - 10000$</option>
															<option value="3">10000$ - 20000$</option>
															<option value="4">more than 20000$</option>
														</select> <i></i> </label>
												</section>
											</div>

											<section class="col col-4">
												<label>Prioridad:</label>	
												<div class="inline-group">
													<label class="radio">
														<input type="radio" name="radio-inline" checked="">
														<i></i>Alta</label>
													<label class="radio">
														<input type="radio" name="radio-inline">
														<i></i>Media</label>
													<label class="radio">
														<input type="radio" name="radio-inline">
														<i></i>Baja</label>
												</div>
											</section>
										</fieldset>
										{{/if}}
										<footer>
											<button type="submit" class="btn btn-primary">
												Guardar
											</button>
										</footer>
									</form>

								</div>
								<!-- end widget content -->
								
							</div>
							<!-- end widget div -->
							
						</div>
						<!-- end widget -->				

					</article>
					<!-- END COL -->	
				</div>

				<!-- END ROW -->

			</section>
			<!-- end widget grid -->

			</div>
			<!-- END MAIN CONTENT -->

		</div>
		<!-- END MAIN PANEL -->