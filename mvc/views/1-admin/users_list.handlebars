						<h1 class="page-title txt-color-blueDark"><i class="fa-fw fa fa-home"></i> Usuarios <span>> todos los usuarios</span></h1>
					</div>
				</div>
							<!-- widget grid -->
							<section id="widget-grid" class="">

								<!-- row -->
								<div class="row">
									
									<!-- NEW WIDGET START -->
									<article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
										
										<!-- Widget ID (each widget will need unique ID)-->
										<div class="jarviswidget jarviswidget-color-blueDark" id="wid-id-1" data-widget-editbutton="false">
											<header>
												<span class="widget-icon"> <i class="fa fa-table"></i> </span>
												<h2>Todos los usuarios </h2>

											</header>

											<!-- widget div-->
											<div>

												<!-- widget edit box -->
												<div class="jarviswidget-editbox">
													<!-- This area used as dropdown edit box -->

												</div>
												<!-- end widget edit box -->

												<!-- widget content -->
												<div class="widget-body no-padding">

													<table id="datatable_fixed_column" class="table table-striped table-bordered" width="100%">
								
														<thead>
															<tr>
																<th class="hasinput" style="width:10%">
																	<input type="text" class="form-control" placeholder="Ticket No." />
																</th>
																<th class="hasinput" style="width:20%">
																	<input type="text" class="form-control" placeholder="Titulo" />
																</th>
																<th class="hasinput" style="width:15%">
																	<input type="text" class="form-control" placeholder="Cliente" />
																</th>
																<th class="hasinput" style="width:15%">
																	<input type="text" class="form-control" placeholder="Ciudad" />
																</th>
																<th class="hasinput" style="width:15%">
																	<input type="text" class="form-control" placeholder="Tienda" />
																</th>
																<th class="hasinput icon-addon">
																	<input id="dateselect_filter" type="text" placeholder="Fecha" class="form-control datepicker" data-dateformat="yy/mm/dd">
																	<label for="dateselect_filter" class="glyphicon glyphicon-calendar no-margin padding-top-15" rel="tooltip" title="" data-original-title="Purchase Date"></label>
																</th>
																<th class="hasinput" style="width:10%">
																	<input type="text" class="form-control" placeholder="Estado" />
																</th>													
															</tr>
															<tr>
																<th data-class="expand">Empresa</th>
																<th >Username</th>
																<th data-hide="phone,tablet">Nombre</th>
																<th data-hide="phone,tablet">Apellido</th>
																<th data-hide="phone,tablet">Aprobado?</th>								                    
																<th data-hide="phone,tablet">Fecha Creación</th>
																<th >Editar</th>
															</tr>
														</thead>

														<tbody>
															{{#each users}}
															<tr>
																<td align="center">{{userType}}</td>
																<td align="center">{{username}}</td>
																<td align="center">{{name}}</td>
																<td align="center">{{lastname}}</td>
																<td align="center">{{userApproval}}</td>
																<td align="center">{{formatDate createdOn "YYYY/MM/DD"}}</td>
																<td><a style="text-decoration: none" href="/admin/listUsers/editUser?userId={{id}}"><button class="btn btn-info center-block padding-5 label view-ticket"
																>
																	Seleccionar
																</button></a></td>
															</tr>
															{{/each}}
														</tbody>
																			
													</table>

